.main {
  width: 750rpx;
  height: 40rpx;
  display: flex;
  justify-content: space-between;
  padding-bottom: 10rpx;
  border-bottom:1px solid #F5F6F7;
  color: #696969;
  font-size: 15px;
}
.account {
  margin: 10rpx;
  margin-left: 40rpx;
}
.count {
  margin-right: 40rpx;
  width: 100rpx;
  height: 50rpx;
  border-radius: 25rpx;
  color: #fff;
  background-color: #ff9933;
  font-size: 14px;
  text-align: center;
  line-height: 50rpx;
}
/* .count:hover , .account:hover{
  color: #3366ff;
} */
.list {
  display: flex;
  justify-content: space-between;
  height: 150rpx;
  margin-top: 10rpx;
  border-bottom: 1px solid #F5F6F7;
}
.left {
  margin-left: 40rpx;
}
.left .info {
  font-size: 14px;
  margin-bottom: 10rpx;
  color: #9966ff;
}
.left .classification {
  font-size: 13px;
  margin-bottom: 10rpx;
  color: #3399ff;
}
.left .date{
  font-size: 12px;
  color: #ff6600;
}
.right {
  font-weight: 700;
  margin-right: 40rpx;
}